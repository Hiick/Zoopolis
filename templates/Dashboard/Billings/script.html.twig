<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<!-- Core plugin JavaScript-->
<script src="../../vendor/jquery-easing/jquery.easing.min.js"></script>

<!-- Custom scripts for all pages-->
<script src="../../js/sb-admin-2.min.js"></script>
<script src="../vendor/chart.js/Chart.min.js"></script>
<script src="../js/demo/sub-charts.js"></script>

<script>
    $(document).ready(function(){
        $.ajax({
            url : '/users/sub',
            type: 'GET',
            async: true,
            success: function (data) {
                $('#all-sub').html(new Intl.NumberFormat('de-DE', { style: 'currency', currency: 'EUR' }).format(parseFloat(data)));
            }
        });
        $.ajax({
            url : '/users/sub/active',
            type: 'GET',
            async: true,
            success: function (data) {
                $('#sub-active').html(new Intl.NumberFormat().format(parseFloat(data)));
            }
        });
    });
</script>