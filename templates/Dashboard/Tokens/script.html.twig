<script>
    $('.birthday').change(function() {
        $.ajax({
            url : '/tokens/connections',
            type: 'POST',
            dataType: "json",
            data: JSON.stringify({
                "day": $(this).val()
            }),
            async: true
        }).then(function (value) {
            $('#connexions').html(value);
           console.log(value)
        });
    });
</script>